{
    "manifest_version" : 3,
    "name" : "Focus Mode",
    "description" : "Enable focus mode!",
    "version" : "1.0",
    "icons" : {
        "16" : "icon-16.png",
        "32" : "icon-32.png",
        "48" : "icon-48.png",
        "128" : "icon-128.png"

    },
    "action" : {
        "default_icon" : {
            "32" : "icon-32.png"
        }
    },
    "background" : {
        "service_worker" : "background.js"
    },
    "permissions" : ["activeTab","scripting"],
    "commands" : {
        "_execute_action": {
            "suggested_key": {
              "default": "Ctrl+B",
              "mac": "Command+B"
            }
          }
    }
}